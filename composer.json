{
    "name": "hdrm147/price-checker",
    "description": "Price comparison tool for Laravel Nova",
    "keywords": [
        "laravel",
        "nova",
        "price-checker"
    ],
    "license": "MIT",
    "require": {
        "php": "^8.1",
        "illuminate/support": "^10.0|^11.0|^12.0"
    },
    "require-dev": {
        "laravel/nova": "^5.0",
        "laravel/nova-devtool": "^1.8"
    },
    "repositories": [
        { "type": "composer", "url": "https://nova.laravel.com" }
    ],
    "autoload": {
        "psr-4": {
            "Hdrm147\\PriceChecker\\": "src/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Hdrm147\\PriceChecker\\ToolServiceProvider"
            ]
        }
    },
    "config": {
        "sort-packages": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
